<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-catalog="microspringdb" match-name="Roles" />
	<table-filter match-catalog="microspringdb" match-name="Users" />
	<table catalog="microspringdb" name="Users">
		<column name="UserId"></column>
		<column name="Login"></column>
		<column name="Password"></column>
		<column name="RoleId"></column>
	</table>
	<table catalog="microspringdb" name="Roles">
		<column name="RoleId"></column>
		<column name="RoleName"></column>
	</table>
</hibernate-reverse-engineering>